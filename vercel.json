{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm ci --legacy-peer-deps || npm install --legacy-peer-deps",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ]
}
